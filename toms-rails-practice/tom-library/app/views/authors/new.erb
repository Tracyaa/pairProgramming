<h1>New Author</h1>
<%= form_for @author do |f| %>
<ul>
    <% if @author.errors.any? %>
      <% @author.errors.full_messages.each do |message| %>
        <li>
          <%= message %>
        </li>
      <% end %>
    <% end %>
</ul>
<p>
    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :birth_year %>
    <%= f.text_field :birth_year %>
</p>
  <%= f.submit %>
<% end %>
<br>
<%= link_to "Back to all Authors", authors_path %>
